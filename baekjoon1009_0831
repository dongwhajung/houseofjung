#include <iostream>
#include <algorithm>
#include <queue>
#include <vector>
#include <cmath>
using namespace std;
int main()
{
	
	ios::sync_with_stdio(false);
	cin.tie(NULL); cout.tie(NULL);

	int case_n;
	long long num = 1;
	cin >> case_n;
	while (case_n--)
	{
		int a, b;
		cin >> a >> b;
		while (b--)
		{
			num *= a;
			num %= 10;
		}
		if (num == 0)
		{
			cout << "10\n";
		}
		else
		{
			cout << num % 10 << "\n";
		}
		num = 1;
	}
	return 0;

}
